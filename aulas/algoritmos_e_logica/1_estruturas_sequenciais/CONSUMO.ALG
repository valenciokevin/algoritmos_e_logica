Algoritmo "consumo"

Var

   distancia, combustivel, total : real

Inicio

      escreval("Digite distancia percorrida: ")
      leia(distancia)
      escreval("Digite o combustivel gasto: ")
      leia(combustivel)

      total <- distancia / combustivel
      
      escreval("O consumo medio de combustivel foi de ", total:8:3)

Fimalgoritmo